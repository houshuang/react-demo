s","original":{"line":58,"column":8}},{"generated":{"line":115,"column":9},"source":"ReactiveRichText/CustomQuillToolbar.js","original":{"line":26,"column":6}},{"generated":{"line":115,"column":10},"source":"ReactiveRichText/CustomQuillToolbar.js","original":{"line":25,"column":4}},{"generated":{"line":116,"column":0},"source":"ReactiveRichText/CustomQuillToolbar.js","original":{"line":68,"column":3}},{"name":"Component","generated":{"line":120,"column":2},"source":"ReactiveRichText/CustomQuillToolbar.js","original":{"line":20,"column":33}},{"name":"CustomQuillToolbar","generated":{"line":122,"column":15},"source":"ReactiveRichText/CustomQuillToolbar.js","original":{"line":71,"column":15}}],"sources":{"ReactiveRichText/CustomQuillToolbar.js":"// @flow\n\nimport React, { Component } from 'react';\n\n// Add styles for LI+ button in toolbar\nconst menuItemStyle = document.createElement('style');\nmenuItemStyle.type = 'text/css';\nmenuItemStyle.innerHTML = `.ql-insertLi .ql-picker-item:before { content: attr(data-label); }\n      .ql-insertLi .ql-picker-label:before { content: 'LI+'; padding-right: 12px; }`;\ndocument.documentElement // $FlowFixMe\n  .getElementsByTagName('head')[0]\n  .appendChild(menuItemStyle);\n\ntype CustomQuillToolbarPropsT = {\n  id: string,\n  readOnly: Object,\n  liTypes: Object\n};\n\nclass CustomQuillToolbar extends Component<CustomQuillToolbarPropsT> {\n  render() {\n    const { id, readOnly, liTypes } = this.props;\n    const AuthorshipToggleBtn = () => <span>AU</span>;\n\n    return (\n      <div\n        id={`toolbar-${id}`}\n        style={{ display: readOnly ? 'none' : 'block' }}\n      >\n        <button className=\"ql-bold\" />\n        <button className=\"ql-italic\" />\n        <button className=\"ql-underline\" />\n        <button className=\"ql-strike\" />\n\n        <button className=\"ql-blockquote\" />\n        <button className=\"ql-code-block\" />\n\n        <select\n          className=\"ql-header\"\n          defaultValue=\"\"\n          onChange={e => e.persist()}\n        >\n          <option value=\"1\" />\n          <option value=\"2\" />\n          <option defaultValue />\n        </select>\n\n        <button className=\"ql-list\" value=\"ordered\" />\n        <button className=\"ql-list\" value=\"bullet\" />\n\n        <button className=\"ql-link\" />\n        <button className=\"ql-image\" />\n        <button className=\"ql-video\" />\n\n        <button className=\"ql-toggleAuthorship\">\n          <AuthorshipToggleBtn />\n        </button>\n        <select className=\"ql-insertLi\" onChange={e => e.persist()}>\n          <option value=\"\">Select type...</option>\n          {liTypes.map(type => (\n            <option key={`${type.id}-${id}`} value={type.id}>\n              {type.name}\n            </option>\n          ))}\n        </select>\n      </div>\n    );\n  }\n}\n\nexport default CustomQuillToolbar;\n"},"lineCount":null}},"error":null,"hash":"829b13be97f76c8a8ebc8085425f1660","cacheData":{"env":{}}}